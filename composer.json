{
    "name": "nexylan/cloudflare-host-gw",
    "description": "A PHP sdk to communicate with CloudFlare Hosting Provider API",
    "keywords": ["CloudFlare", "API", "SDK", "Hosting Provider"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "sullivan@nexylan.com"
        }
    ],
    "require": {
        "php": "^5.4.0 || ^7.0",
        "kriswallsmith/buzz": "~0.1"
    },
    "require-dev": {
        "symfony/http-kernel": "^2.7",
        "symfony/dependency-injection": "^2.7",
        "symfony/config": "^2.7",
        "sensio/buzz-bundle": "^1.0"
    },
    "suggest": {
        "symfony/http-kernel": "For Symfony integration as bundle",
        "symfony/dependency-injection": "For Symfony integration as bundle",
        "symfony/config": "For Symfony integration as bundle",
        "sensio/buzz-bundle": "For Symfony integration as bundle"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Nexy\\CloudFlareHostGW\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Nexy\\CloudFlareHostGW\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
